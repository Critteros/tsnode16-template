{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "compilerOptions": {
    "lib": ["ESNext"],
    //Strict mode
    "strict": true,
    //Module stuff
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ESNext",
    //Json module resolve
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "outDir": "dist/"
  }
}
